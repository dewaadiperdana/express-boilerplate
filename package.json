{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate to help build express project",
  "main": "app.js",
  "author": "Adi",
  "license": "MIT",
  "scripts": {
    "sequelize": "./node_modules/.bin/sequelize",
    "start:dev": "./node_modules/.bin/nodemon src/app.js --exec babel-node",
    "start": "node dist/app.js",
    "build": "babel src -d dist"
  },
  "_moduleAliases": {
    "@root": "./src",
    "@controllers": "src/controllers",
    "@services": "src/services",
    "@helpers": "src/helpers",
    "@models": "src/database/models",
    "@middlewares": "src/middlewares",
    "@validators": "src/middlewares/validators",
    "@routes": "src/routes"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "module-alias": "^2.2.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.18.1",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.2"
  }
}
