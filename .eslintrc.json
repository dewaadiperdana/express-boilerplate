{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "prettier/babel"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
